@using BancoDeSangre.App_Data
@model BancoDeSangre.ViewModels.CampaignViewModels.CampaignListViewModel

@if (Session.IsSignedIn())
{
    <div class="row">
        <div class="col s12 l3">
            @{
                Html.RenderPartial("PartilalViews/MenuOptions");
            }
        </div>
        <div class="col s12 l9">
            @{
                Html.RenderPartial("PartilalViews/CampaignList");
            }     
        </div>
    </div>
}
else
{
    <div class="container">
        @{
            Html.RenderPartial("PartilalViews/CampaignList");
        } 
    </div>
}


