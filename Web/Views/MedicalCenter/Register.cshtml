<div class="row">
    <div class="col s12 l3">
        @{
            Html.RenderPartial("PartialViewsMed/MenuOptions");
        }
    </div>
    <div class="col s12 l9">
        <div class="container">
            <div class="row">
                <div class="card">
                    <div class="card-content container">
                        <span class="card-title center">Crear Centro Medico</span>
                        <div class="row">
                            <form class="col s12" id="create-medicalCenter-form" method="POST" action="/medicalCenter/SaveMedicalCenter">

                                <div class="row">
                                    <div class="input-field col s12">
                                        <input id="name-field" name="name" type="text" required />
                                        <label for="name-field">Nombre Centro Médico</label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s12">
                                        <input id="email-field" name="email" type="email" required />
                                        <label for="email-field">Correo Electrónico</label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s12">
                                        <input id="phonenumber-field" name="phonenumber" type="tel" placeholder="8888-8888" required />
                                        <label for="phonenumber-field">Teléfono</label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s12">
                                        <input id="name-field" name="name" type="text" required />
                                        <label for="name-field">Lugar</label>
                                    </div>
                                </div>
                                <div class="input-field col s12 white-text">
                                    <input id="submit" type="submit" hidden />
                                    <a id="submit-btn" class="waves-effect waves-light btn teal accent-3" href="javascript:void(0)">
                                        <label class="white-text" for="submit">Registrar Centro Médico</label>
                                    </a>
                                </div>

                            </form>
                            </div>
                        </div>
                    </div>
            </div>
        </div>
    </div>
</div>

@section JavaScript{
  <script type="module" src="/public/javascript/views/medicalcenter/register.js"></script>
  }
